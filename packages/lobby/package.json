		"discord-akairo": "8.0.0",
		"discord.js": "12.1.1",
		"dotenv": "8.2.0",
		"googleapis": "48.0.0",
		"humanize-duration": "3.22.0",
		"jsonwebtoken": "8.5.1",
		"morgan": "1.10.0",
		"node-fetch": "^2.6.0",
		"passport": "0.4.1",
		"passport-jwt": "^4.0.0",
		"passport-oauth2": "^1.4.0",
		"pg": "^7.11.0",
		"reflect-metadata": "0.1.13",
		"restify": "8.5.1",
