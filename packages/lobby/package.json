{
	"name": "@r6ru/lobby",
	"version": "0.0.0",
	"private": true,
	"description": "third generation of r6rutils",
	"repository": "https://github.com/Rainbow6-RUSSIA/rainbow6ru-backend",
	"author": "BadCoder1337",
	"license": "MIT",
	"main": "build/index.js",
	"typings": "build/index.d.ts",
	"scripts": {
		"build": "tsc -p .",
		"build:deps": "lerna run build --parallel --scope=@r6ru/lobby --include-filtered-dependencies",
		"bootstrap": "lerna bootstrap",
		"dev": "yarn build:deps && yarn start:watch",
		"start": "node ./build/index.js",
		"start:dev": "node --inspect ./build/index.js",
		"start:watch": "tsc-watch --onSuccess \"yarn start:dev\"",
		"lint": "eslint */**/*.{js,jsx,ts,tsx} --fix"
	},
	"dependencies": {
		"@r6ru/db": "0.0.0",
		"@r6ru/types": "0.0.0",
